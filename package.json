	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/ShipyardSuite/documentation/issues"
	},
	"homepage": "https://github.com/ShipyardSuite/documentation#readme",
	"dependencies": {
		"cors": "2.8.5",
		"express": "^4.18.2",
		"node-fetch": "^2.6.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-markdown": "4.3.1",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"react-syntax-highlighter": "12.2.1",
		"redis": "3.0.2",
		"semantic-ui-react": "^0.88.2",
		"winston": "3.2.1",
		"winston-redis": "3.1.0"
	},
	"devDependencies": {
		"babel-preset-react": "~6.22",
		"babel-preset-latest": "^6",
		"auto-reload-brunch": "^2",
		"clean-css-brunch": "^2",
		"uglify-js-brunch": "^2",
		"concurrently": "^5.1.0",
		"babel-brunch": "~6.0",
		"brunch": "^2",
		"chai": "^4.2.0",
		"jsdoc": "^3.6.3",
		"mocha": "^7.1.1",
		"nodemon": "^2.0.2"
	}
}
